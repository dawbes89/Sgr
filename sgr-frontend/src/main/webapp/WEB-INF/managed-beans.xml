<?xml version="1.0" encoding="UTF-8"?>
<faces-config xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
   xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd"
   version="2.2">

   <managed-bean>
      <managed-bean-name>teachingStuffPanel</managed-bean-name>
      <managed-bean-class>sgr.app.frontend.teachingStuff.TeachingStuffPanel</managed-bean-class>
      <managed-bean-scope>view</managed-bean-scope>
   </managed-bean>
   <managed-bean>
      <managed-bean-name>announcementPanel</managed-bean-name>
      <managed-bean-class>sgr.app.frontend.announcement.AnnouncementPanel</managed-bean-class>
      <managed-bean-scope>view</managed-bean-scope>
   </managed-bean>
   <managed-bean>
      <managed-bean-name>localeManager</managed-bean-name>
      <managed-bean-class>sgr.commons.frontend.LocaleManager</managed-bean-class>
      <managed-bean-scope>session</managed-bean-scope>
   </managed-bean>

</faces-config>